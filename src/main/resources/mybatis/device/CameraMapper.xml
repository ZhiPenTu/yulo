<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.device.mapper.CameraMapper">
    
    <resultMap type="Camera" id="CameraResult">
        <result property="id"           column="id"           />
        <result property="cameraCode"   column="camera_code"  />
        <result property="cameraName"   column="camera_name"  />
        <result property="videoSource"  column="video_source" />
        <result property="mediaProxy"   column="media_proxy"  />
        <result property="radarCode"    column="radar_code"   />
        <result property="coordinates"  column="coordinates"  />
        <result property="cameraInfo"   column="camera_info"  />
        <result property="status"       column="status"       />
        <result property="createBy"     column="create_by"    />
        <result property="createTime"   column="create_time"  />
        <result property="updateBy"     column="update_by"    />
        <result property="updateTime"   column="update_time"  />
        <result property="remark"       column="remark"       />
        <result property="aiFunction"   column="ai_function"  />
        <result property="regions"      column="regions"      />
        <result property="cameraUrl"    column="camera_url"   />
        <result property="backgroundSnap" column="background_snap" />
        <result property="lanes"        column="lanes"        />
    </resultMap>

    <sql id="selectCameraVo">
        select id, camera_code, camera_name, video_source, media_proxy, radar_code, coordinates, camera_info, status, create_by, create_time, update_by, update_time, remark, ai_function, regions, camera_url, background_snap, lanes from camera
    </sql>

    <select id="selectCameraList" parameterType="Camera" resultMap="CameraResult">
        <include refid="selectCameraVo"/>
        <where>  
            <if test="cameraCode != null  and cameraCode != ''"> and camera_code like concat('%', #{cameraCode}, '%')</if>
            <if test="cameraName != null  and cameraName != ''"> and camera_name like concat('%', #{cameraName}, '%')</if>
            <if test="videoSource != null  and videoSource != ''"> and video_source = #{videoSource}</if>
            <if test="mediaProxy != null  and mediaProxy != ''"> and media_proxy = #{mediaProxy}</if>
            <if test="radarCode != null  and radarCode != ''"> and radar_code = #{radarCode}</if>
            <if test="coordinates != null  and coordinates != ''"> and coordinates = #{coordinates}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
        order by create_time desc
    </select>
    
    <select id="selectCameraById" parameterType="Long" resultMap="CameraResult">
        <include refid="selectCameraVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertCamera" parameterType="Camera" useGeneratedKeys="true" keyProperty="id">
        insert into camera
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="cameraCode != null and cameraCode != ''">camera_code,</if>
            <if test="cameraName != null and cameraName != ''">camera_name,</if>
            <if test="videoSource != null and videoSource != ''">video_source,</if>
            <if test="mediaProxy != null and mediaProxy != ''">media_proxy,</if>
            <if test="radarCode != null">radar_code,</if>
            <if test="coordinates != null">coordinates,</if>
            <if test="cameraInfo != null">camera_info,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="aiFunction != null">ai_function,</if>
            <if test="regions != null">regions,</if>
            <if test="cameraUrl != null">camera_url,</if>
            <if test="backgroundSnap != null">background_snap,</if>
            <if test="lanes != null">lanes,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="cameraCode != null and cameraCode != ''">#{cameraCode},</if>
            <if test="cameraName != null and cameraName != ''">#{cameraName},</if>
            <if test="videoSource != null and videoSource != ''">#{videoSource},</if>
            <if test="mediaProxy != null and mediaProxy != ''">#{mediaProxy},</if>
            <if test="radarCode != null">#{radarCode},</if>
            <if test="coordinates != null">#{coordinates},</if>
            <if test="cameraInfo != null">#{cameraInfo},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="aiFunction != null">#{aiFunction},</if>
            <if test="regions != null">#{regions},</if>
            <if test="cameraUrl != null">#{cameraUrl},</if>
            <if test="backgroundSnap != null">#{backgroundSnap},</if>
            <if test="lanes != null">#{lanes},</if>
         </trim>
    </insert>

    <update id="updateCamera" parameterType="Camera">
        update camera
        <trim prefix="SET" suffixOverrides=",">
            <if test="cameraCode != null and cameraCode != ''">camera_code = #{cameraCode},</if>
            <if test="cameraName != null and cameraName != ''">camera_name = #{cameraName},</if>
            <if test="videoSource != null and videoSource != ''">video_source = #{videoSource},</if>
            <if test="mediaProxy != null and mediaProxy != ''">media_proxy = #{mediaProxy},</if>
            <if test="radarCode != null">radar_code = #{radarCode},</if>
            <if test="coordinates != null">coordinates = #{coordinates},</if>
            <if test="cameraInfo != null">camera_info = #{cameraInfo},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="aiFunction != null">ai_function = #{aiFunction},</if>
            <if test="regions != null">regions = #{regions},</if>
            <if test="cameraUrl != null">camera_url = #{cameraUrl},</if>
            <if test="backgroundSnap != null">background_snap = #{backgroundSnap},</if>
            <if test="lanes != null">lanes = #{lanes},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCameraById" parameterType="Long">
        delete from camera where id = #{id}
    </delete>

    <delete id="deleteCameraByIds" parameterType="String">
        delete from camera where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="checkCameraCodeUnique" parameterType="String" resultMap="CameraResult">
        <include refid="selectCameraVo"/>
        where camera_code = #{cameraCode} limit 1
    </select>
</mapper>